{
  "name": "@unpourtous/react-native-ui-wrapper",
  "version": "0.0.0-beta.12",
  "description": "React Native UI",
  "main": "src/index.js",
  "repository": {
    "url": "git+https://github.com/UnPourTous/react-native-ui.git",
    "type": "git"
  },
  "author": "erichua23 <huatingzl@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib/ lib-rn/",
    "build": "npm run build-docs && npm run build-web && npm run build-rn && npm run build-dot-flow",
    "watch-rn": "mkdir -p lib-rn/views/ && BABEL_ENV=publish-rn babel -w src -d lib-rn",
    "watch-web": "mkdir -p lib/views/ && BABEL_ENV=publish-web babel -w src -d lib",
    "build-rn": "mkdir -p lib-rn/views/ && BABEL_ENV=publish-rn babel src -d lib-rn --ignore __tests__",
    "build-web": "mkdir -p lib/views/ && BABEL_ENV=publish-web babel src -d lib --ignore __tests__",
    "build-dot-flow": "find ./src -name '*.js' -not -path '*/__tests__*' | while read filepath; do cp $filepath `echo $filepath | sed 's/\\/src\\//\\/lib\\//g'`.flow && cp $filepath `echo $filepath | sed 's/\\/src\\//\\/lib-rn\\//g'`.flow; done",
    "build-docs": "node scripts/compile-docs",
    "start": "npm run run-playground-ios",
    "run-playground-ios": "cd examples/NavigationPlayground && npm run ios",
    "run-playground-android": "cd examples/NavigationPlayground && npm run android",
    "test": "jest",
    "codecov": "codecov",
    "jest": "jest",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "flow": "flow",
    "flow-check": "flow check --show-all-errors",
    "prepublish": "npm run clean && npm run build && gulp build:web && git submodule init && git submodule update && cd website && yarn && cd react-native-web-player && yarn && cd ../../ ",
    "website": "cd website && yarn run start"
  },
  "files": [
    "lib",
    "lib-rn",
    "src"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "hoist-non-react-statics": "^2.2.0",
    "jest-cli": "^21.2.1",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.5.10",
    "react-native-drawer-layout-polyfill": "^1.3.2",
    "react-native-tab-view": "^0.0.69",
    "react-native-web": "^0.1.9",
    "webpack-babel-jest": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-react-native-syntax": "^1.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codecov": "^2.2.0",
    "compare-versions": "^3.0.1",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.3.1",
    "flow-bin": "^0.49.1",
    "gulp": "^3.9.1",
    "gulp-install": "^1.1.0",
    "gulp-shell": "^0.6.3",
    "jest": "^20.0.4",
    "node-version-compare": "^1.0.1",
    "prettier": "^1.5.3",
    "prettier-eslint": "^6.4.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-native": "^0.40.0",
    "react-native-vector-icons": "^4.2.0",
    "react-test-renderer": "^15.4.2",
    "standard": "^10.0.3",
    "webpack": "^3.7.1"
  },
  "jest": {
    "notify": true,
    "verbose": true,
    "preset": "react-native",
    "testRegex": "./__tests__/.*\\-test\\.js$",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|tcomb-form-native|react-native-localization|@exponent/react-native-action-sheet|@exponent/ex-navigation|@exponent/react-native-touchable-native-feedback-safe|rnrf-relay-renderer|react-clone-referenced-element|react-native-looped-carousel|rn-splash-screen)"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/webpack-babel-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "envVersion": {
    "nodejs": {
      "min": "4.0.0"
    },
    "git": {
      "min": "2.9.0"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
